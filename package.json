    "build": "run-p build:module build:cjs"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konradkalemba/tabler-icons-react.git"
  },
  "keywords": [
    "react",
    "icons"
  ],
  "author": {
    "name": "Konrad Kalemba",
    "email": "konrad@kale.mba"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konradkalemba/tabler-icons-react/issues"
  },
  "homepage": "https://tabler-icons-react.vercel.app",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/parser": "^7.9.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@tabler/icons": "^1.37.0",
    "husky": "^4.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "rollup": "^2.3.2",
    "rollup-plugin-babel": "^4.4.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
